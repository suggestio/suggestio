*
  margin           0
  padding          0
  border           0
  font-size        100%
  font             inherit
  vertical-align   baseline

article, aside, details, figcaption, figure,
footer, header, hgroup, menu, nav, section
  display          block

html,
body
  height           100%

ol, ul
  list-style       none

blockquote, q
  quotes           none

blockquote:before, blockquote:after,
q:before, q:after
  content          ''
  content          none

table
  border-collapse  collapse
  border-spacing   0

sup, sub
  font-size        75%
  line-height      0
  position         relative

sup
  vertical-align   super

sub
  vertical-align   sub

em
  font-style       italic
