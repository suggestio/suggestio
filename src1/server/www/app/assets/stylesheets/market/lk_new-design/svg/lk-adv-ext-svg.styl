
.social-target

  &.__success
    .social-target_icon
      background-image      url('data:image/svg+xml;base64,')
      background-repeat     no-repeat
      background-position   center

  &.__error
    .social-target_icon
      background-image      url('data:image/svg+xml;base64,')
      background-repeat     no-repeat
      background-position   center


  &_icon
    background-image        url('data:image/svg+xml;base64,')
    background-repeat       no-repeat
    background-position     center

    &.__vk
      background-image      url('data:image/svg+xml;base64,')

    &.__fb
      background-image      url('data:image/svg+xml;base64,')

    &.__tw
      background-image      url('data:image/svg+xml;base64,')
