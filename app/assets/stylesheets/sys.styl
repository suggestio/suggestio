border-radius(v)
  -webkit-border-radius:v
  border-radius:v

box-shadow(v)
  -webkit-box-shadow: v
  -moz-box-shadow:    v
  box-shadow:         v

text-shadow(v)
  -webkit-text-shadow: v
  -moz-text-shadow:    v
  text-shadow:         v

@import "market/reset.styl"
@import "market/fonts.styl"
@import "market/lk/base.styl"
@import "market/lk/buttons.styl"
@import "market/lk/text-styles.styl"

.object
    padding: 5px 20px
    border-left: 2px #ccc solid

.data-row
    padding: 20px 0px

.clear
    clear: both

body
    background: #152d3b

*
    padding: 0px
    margin: 0px
    border: 0px

.sio-db-layout
    background: #f0f0f0
    margin: 20px
    padding: 20px
    font-family: Arial
    font-size: 14px
.sio-db-layout a
    color: #328fb0

.sio-db-logo
    padding: 5px 10px 0px 10px

.db-search-result
    margin: 10px 0px
    padding-bottom: 10px
    border-bottom: 1px #ccc dashed

.db-search-result img
    margin: 10px 10px 10px 0px
    float: left
p
  font-size: 12px
  padding: 10px 0px

a.add
  color: green

table
  th,
  td
    text-align: left
    padding: 10px 20px 10px 0px


/* NEW SYS */

.sys
  font-size: 14px
  font-family: 'Arial'

.sys-strong
  color: #676666
  font-weight: bold

.ovh
  overflow: hidden

.abs
  position: absolute
  top: 0
  left: 0

.f-left
  float: left

.f-right
  float: right

.border-line
  overflow: hidden
  width: 100%
  border-top: 1px solid #cbcbcb
  border-bottom: 1px solid #f8f8f8

.border-line-vertical
  margin-top: 5px
  border-left: 1px solid #cbcbcb
  border-right: 1px solid #f8f8f8


.sys-market-index__items
  overflow: hidden
  margin-top: 60px

.sys-market-index__item
  overflow: hidden
  float: left
  width: 130px
  margin-right: 35px
  margin-bottom: 35px

.sys-market-index__btn
  margin-top: 15px

.free-ad__link
  display: block
  margin: 5px 0


.info-label
  overflow: hidden

.info-label._market-index
  margin-left: -30px
  margin-top: 20px

.info-label__icon
  float: left

  width: 7px /* Ширина и высота по размеру изображения */
  height: 38px

  background: url('/assets/images/market/lk/info-label-icon.png') no-repeat left center

.info-label__title
  padding-left: 25px
  float: left

  font-family: 'Arial'
  font-size: 12px
  font-weight: bold
  color: #959595
  text-shadow(1px 1px 1px white)


.sys__t
  margin: 25px 0

  font-family: 'PT_Sans-Narrow'
  ft(24px)
  color: #676666