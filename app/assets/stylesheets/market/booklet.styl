@import "reset"
@import "mixins"
@import "lk_new-design/buttons"

font-face( name )
  @font-face
    font-family: name
    src: url('/assets/fonts/market/' + name + '.eot')
    src: url('/assets/fonts/market/' + name + '.eot?#iefix') format('embedded-opentype'),
         url('/assets/fonts/market/' + name + '.woff') format('woff'),
         url('/assets/fonts/market/' + name + '.ttf') format('truetype')
    font-weight: normal
    font-style: normal

screen768 = 'screen and (max-width: 767px)'
screen1024 = 'screen and (max-width: 1024px)'
screenGorizontalIpad = 'screen and (max-width: 1024px) and (orientation : landscape)'
screenVerticallIpad = 'screen and (max-width: 1024px) and (orientation : portrait)'
screenIphone4 = 'screen and (max-width: 767px) and (device-aspect-ratio: 2/3)'
screenIphone5 = 'screen and (max-width: 767px) and (device-aspect-ratio: 40/71)'
screen1200 = 'screen and (max-width: 1200px)'
retina = "only screen and (-Webkit-min-device-pixel-ratio: 1.5), only screen and (-moz-min-device-pixel-ratio: 1.5), only screen and (-o-min-device-pixel-ratio: 3/2), only screen and (min-device-pixel-ratio: 1.5)"


font-face( 'opensans-light' )
font-face( 'opensans-bold' )
font-face( 'heliosthin' )

body
  min-width     1024px
  color         white
  font-family   'opensans-light'
  font-size     14px
  height        100% !important

  /* во всех кнопках uppercase */
  .btn
    text-transform  uppercase

.title
  font-family 'heliosthin'
  font-size    40px

.link
  color           #00b994
  text-decoration none
  cursor          pointer

.hidden
  display none

/********************************************************************************************************************/
/* Иконки */
/********************************************************************************************************************/
.ic-suggestio
  display             inline-block
  min-width           55px
  min-height          55px

  cursor              pointer
  background-image    url('data:image/svg+xml;base64,')
  background-repeat   no-repeat
  background-position center

.ic-android
  min-width           10px
  background-image    url('data:image/svg+xml;base64,')
  background-repeat   no-repeat
  transition(none)

  &.__white
    background-position center 6px

    &:hover
      background-position center -33px

  &.__gray
    background-position center -72px

  &.__gray:hover,
  &.__gray.__js-act
    background-position center 6px

.ic-ios
  min-width           10px
  background-image    url('data:image/svg+xml;base64,')
  background-repeat   no-repeat
  transition(none)

  &.__white
    background-position 3px 8px

    &:hover
      background-position 3px -33px

  &.__gray
    background-position center -74px

  &.__gray:hover,
  &.__gray.__js-act
    background-position center 6px


/********************************************************************************************************************/
/* Header */
/********************************************************************************************************************/

.header
  position  absolute
  top       0
  left      0
  width     100%
  z-index   1000

  min-width 1024px
  height    70px

  transition(all 0.4s linear 0s)

  &_cnt
    position  relative
    margin    0 auto
    max-width 1024px

  &_logo
    position    absolute
    top         25px
    left        0

  &_nav
    position  absolute
    left      110px
    top       30px

  &_phone
    position    absolute
    top         30px
    right       210px
    font-family 'opensans-bold'

  &_btn
    position  absolute
    top       20px
    right     0

    padding-bottom    6px
    padding-top       8px
    padding-left      15px
    padding-right     15px
    cursor  pointer

    background-color  rgba(255, 255, 255, 0.25)
    border            1px solid #f3f5f5
    color             #ffffff

    transition( all 0.2s linear 0s)
    border-radius(4px)

    &:hover
      background-color  rgba(255, 255, 255, 0.5)

  &_btn-2
    position  absolute
    top       20px
    right     0

.header.__slide
  position  fixed
  top       -61px
  right     0
  z-index   10000

  height            60px
  border-bottom     1px solid #dadfe1

  color             #b0babf
  background-color  white
  transition(0.5s all ease 0s)

  .header_logo
    top 15px

  .header_nav
    top 20px

  .header_phone
    top 20px

  .header_btn-2
    top 10px

  .top-menu_i
    color #b0babf

  .top-menu_i:hover,
  .top-menu_i.__js-act
    color rgba(79, 89, 98, 0.75)

.header.__slide.__visible
  transform(translate3d(0, 61px, 0))



.top-menu
  overflow hidden /* внутри элементы со свойством float */
  text-transform  uppercase

  &_i
    float       left
    display     inline-block
    padding     0 20px

    color           white
    font-size       13px
    font-weight     300
    text-align      center
    text-decoration none

    &:hover
      color #d5d5d5

  &_i.__js-act
    color #d5d5d5


/********************************************************************************************************************/
/* Секция Ваша реклама */
/********************************************************************************************************************/
.your-adv
  height            100%
  min-height        700px
  max-height        1081px /* высота изображения бэкграунда */
  text-align        center

  background-image      url('/assets/images/market/booklet/section-sio-market_bg.jpg')
  background-size       cover
  background-attachment fixed


  &_cnt
    margin      0 auto
    width       500px
    padding-top 150px

    font-family 'heliosthin'
    font-size   30px
    line-height 30px

  &_t
    margin-bottom 50px

  &_geo
    margin  40px auto 0
    display block
    width   25px
    height  25px

    background-image    url('data:image/svg+xml;base64,')
    background-repeat   no-repeat
    background-position center

  &_text
    margin-top  40px

  &_text-major
    font-size 27px

  &_suggest-btn
    margin          25px 0
    padding-bottom  10px

    background-image    url('data:image/svg+xml;base64,')
    background-repeat   no-repeat
    background-position bottom  center

  &_footer-text
    margin-top  20px
    font-size   12px


/********************************************************************************************************************/
/* Секция Ios/Android */
/********************************************************************************************************************/
.phones
  position        relative
  margin-top      -25px
  padding-top     25px
  padding-bottom  60px
  color           #adadad
  text-align      center

  &_triangle
    position      absolute
    top           0
    left          50%
    margin-left   -50px

    width         0
    height        0
    border-style  solid
    border-width  0 50px 25px 50px
    border-color  transparent transparent #ffffff

  &_title
    margin-top    30px
    margin-bottom 30px
    font-size     18px
    color         #060606

  &_btns
    margin      25px auto
    overflow    hidden
    width       190px
    border      1px solid #00b994
    border-radius(4px)

  &_btn
    display     block
    float       left
    width       50%
    height      40px
    cursor      pointer

  &_btn:hover,
  &_btn.__js-act
    background-color  #00b994

  &_step-lst
    margin    0 auto
    overflow  hidden
    width     1024px

  &_step-i
    float       left
    width       25%
    text-align  center

    background-image    url('data:image/svg+xml;base64,')
    background-repeat   no-repeat
    background-position center right

  &_step-i.__last
    background-image  none

  &_step-img
    position  relative
    height    320px

  &_step-img.__big
    height  340px

  &_step-t
    margin-bottom 30px
    font-size     18px
    color         #060606

  &_step-major-text
    color #292929

/********************************************************************************************************************/
/* Секция Стоимость */
/********************************************************************************************************************/
.price
  overflow  hidden

  &_green
    text-align        center
    padding           15px 0
    background-color  #00b994

  &_t
    display     inline-block
    padding     15px 35px
    border      2px solid white
    text-align  center

  &_white
    position          relative
    padding-bottom    65px
    background-color  white

  &_cnt
    position  reelative
    margin    0 auto
    width     1024px
    padding   40px
    color     #b1b1b1

  &_fingers
    position    absolute
    top         -35px
    left        50%
    margin-left -280px

    width     1024px
    height    737px

    background-image    url('/assets/images/market/booklet/phone_lacation_retina.png')
    background-repeat   no-repeat
    background-position right center
    background-size     auto  737px

  &_text
    width         460px
    padding-left  20px

  &_text.__major
    margin-bottom 40px
    line-height   40px
    color         #656565
    font-family   'heliosthin'
    font-size     24px

  &_map
    height          250px
    border-top      1px solid #e1e1e1
    border-bottom   1px solid #e1e1e1

    background-image  url('/assets/images/market/booklet/map_retina.jpg')
    background-size   auto 250px

  &_map-inner
    margin  0 auto
    padding 0 20px
    width   984px

  &_map-text
    padding   40px 0
    color     #464646
    font-size 24px

/********************************************************************************************************************/
/* Секция Индивидуальная формировка */
/********************************************************************************************************************/
.individual

  &_red
    padding           50px 0
    border            1px solid #e1e1e1
    background-color  #9d8080

  &_red-inner
    margin      0 auto
    width       1024px
    text-align  center

  &_white
    position          relative
    margin            0 auto
    width             1024px
    padding           25px 0
    background-color  white

  &_cnt
    margin  0 auto
    width   1024px
    counter-reset list

  &_text-i:before
    float             left
    padding-right     20px

    font-family       'heliosthin'
    color             #494949
    font-size         36px

    counter-increment list
    content           counter(list) ". "

  &_text
    margin-bottom 30px
    overflow      hidden
    color         #494949
    font-size     18px

  &_ipad
    position  absolute
    bottom    0
    left      340px

    width     1000px
    height    527px

    background-image  url('/assets/images/market/booklet/ipad-map_retina.png')
    background-size   auto 527px

  &_footer-text
    color       #b1b1b1
    text-align  center

/********************************************************************************************************************/
/* Секция Уровни размещения */
/********************************************************************************************************************/
.levels
  padding-bottom  25px

  &_blue
    height            280px
    padding-top       20px
    background-color  #23969e
    text-align        center

  &_white
    position  relative
    height    400px

  &_phone
    position    absolute
    top         -150px
    left        50%
    margin-left -530px

    width       1024px
    height      401px

    background-image    url('/assets/images/market/booklet/phone_screen_retina.png')
    background-size     auto 401px
    background-repeat   no-repeat
    background-position center

  &_phone-text
    position  absolute
    color     #393939
    font-size 18px

  &_phone-text.__left
    top   221px
    left  110px

  &_phone-text.__right
    top   193px
    right -5px

  &_phone-text.__bottom
    bottom  -15px
    left    470px

  &_cnt
    position    relative
    top         300px
    text-align  center

  &_text
    padding-left  40px
    color         #464646
    font-size     18px

    background-image    url('data:image/svg+xml;base64,')
    background-repeat   no-repeat
    background-position -50px center

  &_btn
    padding 30px 0

/********************************************************************************************************************/
/* Slider */
/********************************************************************************************************************/
.bx-pager
  position  static !important
  margin    40px 0 !important

.bx-pager-link
  width               15px !important
  color               white !important
  background-color    transparent !important

  background-image    url('data:image/svg+xml;base64,') !important
  background-repeat   no-repeat !important
  background-position 0 -3px !important
  border-radius(0 !important)

  &.active
    background-position 0 -21px !important

.bx-controls-direction
  position    absolute
  bottom      40px
  left        50%
  margin-left -175px

  display     block
  width       350px

.bx-prev
  width             40px !important
  height            40px !important
  top               5px !important
  margin-top        0 !important
  opacity           1 !important
  background-color  transparent !important

  background-image    url('data:image/svg+xml;base64,') !important
  background-repeat   no-repeat !important
  background-position 0 0 !important

  &:hover
    background-position 0 -50px !important

.bx-next
  width             40px !important
  height            40px !important
  top               5px !important
  margin-top        0 !important
  opacity           1 !important
  background-color  transparent !important

  background-image    url('data:image/svg+xml;base64,')
  background-repeat   no-repeat !important
  background-position 0 0 !important

  &:hover
    background-position 0 -50px !important



