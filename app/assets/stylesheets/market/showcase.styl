@import 'fonts.styl'
@import 'blocks.styl'

spriteurl = url('/vassets/images/sio-market-sprite.png')
spritesize = 400px 100px

sprite2url = url('/vassets/images/showcase-v2-sprite-retina.png')
sprite2size = 150px 150px

screenVerticaliPhone = 'screen and (max-width: 400px)'

default_color_w_opacity = rgba( 48, 48, 48, 0.9 )
default_color = rgba( 48, 48, 48, 1 )

border-radius( param )
  -webkit-border-radius : param
  -moz-border-radius : param
  -o-border-radius : param
  border-radius : param

border-radius-left( param )
  border-top-left-radius            param
  -moz-border-radius-topleft        param
  -webkit-border-top-left-radius    param

  border-bottom-left-radius            param
  -moz-border-radius-bottomleft        param
  -webkit-border-bottom-left-radius    param

border-radius-right( param )
  border-top-right-radius            param
  -moz-border-radius-topright        param
  -webkit-border-top-right-radius    param

  border-bottom-right-radius            param
  -moz-border-radius-bottomright        param
  -webkit-border-bottom-right-radius    param

transform( param )
  -webkit-transform : param
  -moz-transform : param
  -o-transform : param
  -ms-transform : param
  transform : param

transform-origin( param )
  -webkit-transform-origin : param
  -moz-transform-origin : param
  -o-transform-origin : param
  -ms-transform-origin : param
  transform-origin : param

box-shadow( param )
  -webkit-box-shadow: param
  -moz-box-shadow: param
  box-shadow: param

body,
html
  height: 100%
  width: 100%

*
  padding: 0px
  margin: 0px
  border: 0px

.abs
  position: absolute
  left: 0px
  top: 0px

.hidden
  display: none

.sm-clear
  clear: both

.sm-centered-block
  margin: 0px auto
  position: relative

code
  display: none

.sm-body
  background    #3c2a46 no-repeat center center

  &.__ru
    background-image: url('data:image/svg+xml;base64,');
    background-size 120px 67.5px

  &.__en
    background-image: url('data:image/svg+xml;base64,');
    background-size 124.5px 95.25px


.sm-welcome-ad
  position          absolute
  left              0px
  top               0px
  width             100%
  height            100%
  z-index           999
  background        default_color
  overflow          hidden
  display           table
  vertical-align    middle
  text-align        center

  &.__animated
    -webkit-transition: all 0.2s linear
    -moz-transition: all 0.2s linear
    transition: all 0.2s linear
  &.__fade-out
    opacity: 0
  &.__will-fade-out
    will-change:    opacity
  &_helper
    display         inline-block
    height          100%
    vertical-align  middle
    margin-bottom   24px
  &_bg-img,
  &_fg-img
    position        absolute
    left            50%
    top             50%
  &_fg-text
    width           100%
    z-index         100000000000
    position        absolute
    top             50%

.sm-vflex
  display: -webkit-box
  -webkit-box-orient: vertical
  -webkit-box-align: stretch
  -webkit-box-flex:1
  display: -moz-box
  display: -ms-flexbox
  display: -webkit-flex
  display: flex

.sm-overflow-scrolling
  -webkit-overflow-scrolling: touch
  overflow-y: scroll
  margin-bottom: -1px

.sm-block
  position: absolute
  transition:         transform 0.2s linear
  -webkit-transition: transform 0.2s linear
  -moz-transition:    transform 0.2s linear

  &.__rel
    position: relative
    margin: 0px auto
    border: 0px

.sm-b-spacer
  position: absolute
  background: transparent

/*
------------------------------------*/
.fs-animated-start
  transform: translate3d(100%,0,0)
  -moz-transform: translate3d(100%,0,0)
  -webkit-transform: translate3d(100%,0,0)

.fs-animated-end
  transform: translate3d(0,0,0)
  -moz-transform: translate3d(0,0,0)
  -webkit-transform: translate3d(0,0,0)

.transition-animated
  transition: all 0.2s linear
  -webkit-transition: all 0.2s linear
  -moz-transition: all 0.2s linear

.will-translate3d
  will-change: translate3d
  -moz-will-change: translate3d
  -webkit-will-change: translate3d

/* Кнопки
------------------------------------*/
.sm-button
  width: 40px
  height: 40px
  cursor: pointer

.sm-flat-btn
  cursor          pointer
  display         block
  text-decoration none
  padding         10px
  &.__color-1
    background: #f44040
    color: #fff
  &.__centered
    width: 150px
    text-align: center
    margin: 0px auto
  &.__rounded
    border-radius( 10px )

.sm-showcase
  width: 100%
  height: 100%

.js-hdr-logo
  cursor          pointer

.sm-producer-header
  height: 50px
  width: 100%
  background: default_color_w_opacity
  z-index: 10
  text-align: center
  border-bottom    : 1px solid
  -webkit-transform: translate3d(0,0,0)

  &_btn
    &.__left-aligned
      left: 5px
      top: 5px
    &.__right-aligned
      left: auto
      top: 5px
      right: 5px

  &_logo
    padding-top: 10px

  &_txt-logo
    position        relative
    font-family     'opensans-regular'
    font-size       12px
    line-height     50px
    text-transform  uppercase
    color           #fff
    border          1px #fff solid
    border-radius(  3px )
    padding         4px 5px
    &-dot
      background    #fff
      width         6px
      height        6px
      border-radius(50px)
      position      absolute
      left          -12px
      top           9px
      &.__right
        left        auto
        right       -12px
  &_icon
    display: none

  &_exit-button
    display: block

  &_index-button
    display: none

  &_geo-button
    top         7px!important

  &.__w-global-cat
    .sm-producer-header_exit-button,
    .sm-producer-header_geo-button,
    .sm-producer-header_logo,
    .sm-producer-header_txt-logo
      display: none!important

    .sm-producer-header_index-button,
    .sm-producer-header_search-button
      display: block!important

    .sm-producer-header_icon
      display: block!important

  &.__w-index-icon
    .sm-producer-header_exit-button,
    .sm-producer-header_geo-button,
    .sm-producer-header_search-button
      display: none!important

    .sm-producer-header_index-button
      display: block

.__header-icon
  display       none
  margin        0px auto
  height        40px
  margin-top    5px


.fs-loader
  display       none
  position      fixed
  left          0px
  top           0px
  width         100%
  height        100%
  z-index       100
  &_spinner
    position    absolute
    left        50%
    top         50%
    margin-left -16px
    margin-top  -16px

/*
------------------------------------*/
.sm-close-screen
  display: none
  position: absolute
  left: 0px
  top: 0px
  width: 100%
  height: 100%
  z-index: 100
  font-family: 'opensans-light'
  font-size: 14px
  color: #737373

  &_cnt
    position: absolute
    left: 0px
    top: 50px
    width: 100%
    height: 50%
    min-height: 260px
    background: rgba(48, 48, 48,0.95)

  &_exit
    left: 7px
    top: -45px

  &_message
    text-align: center
    padding: 100px 50px 30px 50px

  &_dm
    margin: 25px 0px

.sm-grid-ads
  width: 100%
  height: 100%
  &_content
    position: relative
  &_container
    margin: 0px auto
    position: relative
    overflow: hidden
  &_loader
    position    relative
    height      10px
    margin      0px auto 40px auto
  &_loader-spinner
    position    absolute
    left        50%
    top         50%
    margin-top  -18px
    margin-left -46px
    background  #fff
    transform(scale3d(0.7,0.7,0.7))
  &_loader-spinner-inner
    padding     0px 30px

/*
------------------------------------*/
.sm-categories-screen
  display     none
  position    absolute
  right       0px
  top         0px
  z-index     11
  width       320px
  height      100%

  &.__compact
    width: 300px
    left: 50%

  &.__search-mode
    height: 120px
    overflow: hidden
    .sm-categories,
    .sm-shops,
    .sm-categories-screen_tabs
      display: none!important

  &_header
    margin-top  50px
  &_tabs
    width       100%
    height      50px
    background  default_color_w_opacity
  &_tabs-wrapper
    width       260px
    position    relative
    margin      0px auto
    background  red
  &_single-tab
    width       50%
    float       left
    text-align  center
    font-family 'helios-thin'
    font-size   14px
    cursor      pointer

.sm-tab
  color         #343434
  border        1px #c2c2c2 solid
  text-align    center
  background    #c2c2c2
  padding       8px 0px

  &.__rounded-left
    border-radius-left( 5px )
    border-right        0px

  &.__rounded-right
    border-left         0px
    border-radius-right( 5px )

  &.__inactive
    background  none!important
    opacity     0.9!important
    color       #959595

.shops-list
  font-family       'opensans-light'
  width             100%
  text-align        center
  color             #b1b0b0
  padding-top       10px
  padding-bottom    50px
  &_row
    cursor          pointer
    padding         13px 0px
    font-size       14px
    margin          0px auto
    &.__even
      background     default_color_w_opacity
    &.__selected
      font-family   'opensans-bold'
      font-size     larger

.geo-nodes-list
  width             100%
  background        none
  z-index           10
  &_layer
    opacity         0.7
    border-top      1px rgba(255,255,255,0.5) dotted
    cursor          pointer
    background      none
    &.__active
      opacity       1

      .geo-nodes-list_layer-icon
        display     block
      .geo-nodes-list_layer-icon-right
        display     none

    &.__bo-border
      border-top-color    transparent
  &_layer-inner
    position        relative
    width           260px
    margin          0px auto
    font-family     'opensans-light'
    font-size       14px
    padding         12px 0px
    text-transform  uppercase
  &_layer-icon
    display         none
  &_layer-icon,
  &_layer-icon-right
    position        absolute
    left            -30px
    top             3px
  &_layer-counter
    font-family     'opensans-bold'
    font-size       13px
    float           right
  &_rows
    &.__hidden
      display       none
  &_row
    padding         14px 0px
    text-align      center
    color           #fff
    font-size       14px
    font-family     'opensans-light'
    &.__no-border
      border-bottom 0px

.geo-nodes-links
  height        40px
  width         100%
  position      absolute
  left          0px
  bottom        0px
  font-family   'opensans-light'
  font-size     12px
  line-height   40px
  &_text
    opacity     0.6
  &_inner
    width       260px
    margin      0px auto
  &_right-aligned
    float       right
  &_link
    position    relative
    text-decoration none
    color       #fff
  &_icon
    position            absolute
    width               9px
    height              13px
    left                -13px
    top                 2px
    background-position -3px -3px
    background-image    url('data:image/svg+xml;base64,');
    background-repeat   no-repeat
    path
      fill              red
  &_icon-info
    top                 13px
    left                -10px
    background-position -3px -21px
/*
------------------------------------*/
.sm-geo-screen
  display: none
  position: absolute
  left: 0px
  top: 0px
  width: 320px
  height: 100%
  z-index: 100
  &_header
    height: 50px
  &_search
    height: 55px
  &_search-input
    border: 1px #fff solid
  &_current-location
    padding: 20px 0px
    position: relative
    text-align: center
    color: #fff
    font-family: 'opensans-light'
    font-size: 14px
    background: default_color_w_opacity
    cursor: pointer

  &_current-location-inner
    display         inline-block
    position        relative
    width           260px
    border          1px #fff dotted
    border-radius(  5px )
    font-family     'opensans-bold'
    font-size       12px
    padding         10px 0px

  &_current-location-icon,
  &_current-location-spinner
    position    absolute
    left        0px
    top         1px

  &_current-location-spinner
    display     none
    transform( scale3d(0.5, 0.5, 0.5) )
    left        0px
    top         3px

.sm-geo-nodes
  background: default_color
/*
------------------------------------*/
.sm-search-bar
  height: 35px
  padding: 15px 22px 15px 22px
  background: default_color_w_opacity

  &_field
    position          relative
    width             258px
    margin            0px auto
    height            34px
    border            1px #636363 dotted
    background        default_color_w_opacity
    border-radius(    5px)
    font-family:      'opensans-bold'
    &.__active
      background: #fff!important
      .sm-search-bar_input
        background: #fff

  &_wrapper
    padding: 0px 5px

  &_message
    display         none
    position        absolute
    left            -1px
    bottom          -50px
    line-height     12px
    font-size       11px
    padding         11px 6px
    height          14px
    width           248px
    background      #363636
    color           #fff
    font-family     'opensans-light'
    z-index         10
    border-radius(  5px )

  &_input
    width: 100%
    line-height: 34px
    background: none
    outline: none
    color: #999

/*
------------------------------------*/
.sm-categories
  background: default_color
.sm-shops
  background: default_color


/*
------------------------------------*/
.focused-container
  margin: 0px auto

.sm-focused-ads
  position: absolute
  left: 0px
  top: 0px
  z-index: 215
  width: 100%
  height: 100%
  display: none
  overflow: hidden
  box-shadow( 0px 0px 10px #000 )
  &_arrow-label
    position      absolute
    left          -100px
    top           -100px
    z-index       10
    background    #fff
    color         #000
    padding       8px
    border-radius 50px

  &_container
    width: 100%
    height: 100%
    overflow: visible
    &.__animated
      -webkit-transition: all 0.2s linear
      -moz-transition: all 0.2s linear
      transition: all 0.2s linear

.sm-focused-ads-loader
  width             600px
  height            600px
  background        #ededed
  background-image  url('data:image/jpeg;base64,')
  background-size   cover
  &.__small
    width     300px
    height    300px
  &_icon
    padding-bottom  10px
  &_content
    font-size       12px
    position        absolute
    left            50%
    top             50%
    width           300px
    margin-left     -150px
    margin-top      -30px
    text-align      center
    font-family     "helios-thin"
    font-size       17px
    text-transform  uppercase


.fad-arrow-label
  cursor          pointer
  &.__left-arrow
    .fad-arrow-label_left-arrow
      display: block
    .fad-arrow-label_right-arrow
      display: none

  &.__right-arrow
    .fad-arrow-label_right-arrow
      display: block
    .fad-arrow-label_left-arrow
      display: none

.focused-ad
  float: left

  &_top-offset,
  &_bottom-offset
    height: 50px

  &_offer
    padding: 0px

  &_location
    position: relative
    background: default_color_w_opacity
    color: #fff
    margin: 2px 0px
    padding: 15px 11px

  &_location-icon
    width: 40px
    height: 37px
    float: left
    padding-right: 0px

  &_location-info
    font-family   'opensans-light'
    font-size     14px
    float         left
    max-width     180px
    div
      font-size   12px

  &_count
    position: absolute
    right: 8px
    top: 19px
    border-radius: 50px
    color: #fff
    padding: 16px 0px
    width: 48px
    height: 16px
    text-align: center
    margin-top: -12px
    font-size: 15px
    border: 2px #fff solid
    font-family: 'opensans-light'
    font-size: 12px

  &_descr
    margin      0 auto
    box-sizing  border-box
    max-width   620px

    font-family: Arial
    font-size: 16px
    padding: 40px


.sm-body.__sync

  .sm-grid-ads_container > a
    float left
    margin  20px

    display block

  .sm-block
    position  relative


/* Media
------------------------------------*/
@media screenVerticaliPhone

  .sm-search-bar
    padding-left: 11px
    padding-right: 11px

  .focused-ad
    &_offer
      padding: 0px
    &_location-info
      max-width 180px

  .sm-producer-header
    &.__w-global-cat
      .sm-producer-header_search-button
        display: block!important


.dotted-pattern
  position  absolute
  top     50px
  left    0
  width   100%
  background-repeat repeat

.mvbl-title,
.mvbl-descr
  z-index 5


.display-block
  display: block

