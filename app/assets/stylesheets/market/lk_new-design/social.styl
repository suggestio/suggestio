@import "mixins"

.social-target
  padding 15px 10px
  background-color  white
  border-radius 10px

  &_name
    float left
    box-sizing border-box
    width 70%
    padding-right 25px

  &_option-lst
    border-left 1px solid #e9e9e9
    padding-left 25px
    overflow hidden

  &_option
    margin 10px
    display block
