border-radius(v)
  -webkit-border-radius:v
  border-radius:v

box-shadow(v)
  -webkit-box-shadow: v
  -moz-box-shadow:    v
  box-shadow:         v

text-shadow(v)
  -webkit-text-shadow: v
  -moz-text-shadow:    v
  text-shadow:         v


transition(v)
  -webkit-transition: v
  transition: v

transform(v)
  -webkit-transform: v
  transform: v

@css {
  @import url(http://fonts.googleapis.com/css?family=PT+Sans+Narrow:700,400&subset=cyrillic-ext,latin-ext);

}

@font-face
    font-family: 'ParagNorCIta'
    src: url('/assets/fonts/market/DINCondensedC/PFDinTextCompPro-Medium.eot')
    src: url('/assets/fonts/market/DINCondensedC/PFDinTextCompPro-Medium.eot?#iefix') format('embedded-opentype'),
         url('/assets/fonts/market/DINCondensedC/PFDinTextCompPro-Medium.woff') format('woff'),
         url('/assets/fonts/market/DINCondensedC/PFDinTextCompPro-Medium.ttf') format('truetype'),
         url('/assets/fonts/market/DINCondensedC/PFDinTextCompPro-Medium.svg#pf_din_text_comp_promedium') format('svg')
    font-weight: normal
    font-style: normal

.superuser-link
  font-size: 10px
  color: #999

.lk
  .body
    font-size: 14px
    font-family: 'Arial'
    color: #676666

  .ovh
    overflow: hidden

  .border-line
    overflow: hidden
    width: 100%
    border-top: 1px solid #cbcbcb
    border-bottom: 1px solid #f8f8f8

  .input-wrap
    margin-top: 10px
    margin-bottom: 10px
    .left
      vertical-align: middle
      width: 120px
      label
        padding-right: 30px
        float: left
        font-size: 12px
        color: #959595
        font-family: 'Arial'
    .select-color
      vertical-align: middle
      font-size: 12px
      color: #959595
      font-family: 'Arial'
      padding-left: 20px
      width: 80px
      .color
        margin-left: 10px
        display: inline-block
        width: 27px
        line-height: 27px
        border: 1px solid #b2b2b2
        border-radius(2px)
      .color.custom
        background: url('/assets/images/market/lk/custom-color.png') no-repeat
        background-size: 21px 21px
        background-position: 3px 3px
    .close
      margin-top: 5px
      margin-right: 10px
  .input-wrap.textarea
    .left
      vertical-align: top
      padding-top: 18px
  .input-wrap.big
    width: 100%
  .input-wrap.small
    width: 260px
    float: left
    margin-right: 60px
  .input-wrap.hide-label
    .left
      display: none
  .input
    background: none repeat scroll 0 0 #FFFFFF
    border: 1px solid #B4B3B3
    padding: 1px
    .input-shadow
      border-left: 1px solid #FBFBFB
      border-right: 1px solid #FBFBFB
      border-top: 1px solid #D2D3D3
      border-radius(4px)
      input
        outline: 0
        padding: 0 10px
        width: 100%
        height: 35px
        font-size: 16px
        color: #959595
        font-family: 'Arial'
      textarea
        width: 100%
        height: 170px
        padding: 10px
        resize: none
        font-size: 16px
        color: #959595
        font-family: 'Arial'
  .focus
    .input
      border: 1px solid #07a1ca !important
      .input-shadow
        input
          color: #07a1ca
        textarea
          color: #07a1ca
  .input.error
    border: 1px solid #e23131
  .input-wrap.disabled
    input
      background: url('/assets/images/market/lk/lock.png') no-repeat 95% center #F0F0F0
  .input-wrap.big.disabled
    input
      background: url('/assets/images/market/lk/lock.png') no-repeat 99% center #F0F0F0
  .input-wrap.pass
    td
      padding: 5px 0
    .input
      width: 278px
    .pass-text
      vertical-align: middle
      font-size: 12px
      font-family: 'Arial'
      color: #b4b4b4
      width: 250px
      padding-left: 45px
  .mce-container
    font-size: 16px !important
    color: #959595 !important
    font-family: 'Arial' !important

  .green-but-big
    outline: 0
    display: block
    float: right
    width: 210px
    height: 14px
    color: white
    cursor: pointer
    font-size: 14px
    line-height: 14px
    text-decoration: none
    text-align: center
    padding: 13px 0
    font-family: 'Arial'
    background: url('/assets/images/market/lk/green-but.big.png') no-repeat
    background-position: 0 -180px
  .green-but-big:hover
    background-position: 0 -90px
  .green-but-big:active
    color: #dcea8
    background-position: 0 0


  .gray-but-wrap
    border: 1px solid #9a9b9b
    border-radius(6px)
    display: inline-block
    padding: 1px 0
    .white-border-top
      border-radius(6px)
      box-shadow(0 -1px 0 0 white)
      .btn
        background: none
        border: none
        display: inline-block
        padding: 16px 18px
        color: #676666
        cursor: pointer
        font-size: 30px
        text-decoration: none
        text-align: center
        font-family: 'ParagNorCIta'
        box-shadow(0 1px 0 0 #B5B6B6)
        text-shadow(1px 1px 1px #FFFFFF)
  .gray-but-wrap:hover
    border: 1px solid #4f88a9
    .btn
      color: #4f88a9
  .gray-but-wrap:active
    border: 1px solid #9a9b9b
    box-shadow(inset 0 0 40px 0 #9a9b9b)
    .white-border-top
      box-shadow: none
      .btn
        color: #676666


  .green-but-small
    outline: 0
    display: inline-block
    float: right
    width: 100px
    padding: 13px 0
    color: white
    cursor: pointer
    text-align: center
    font-size: 14px
    line-height: 14px
    font-family: 'Arial'
    background: url('/assets/images/market/lk/green-but.small.png') no-repeat
    background-position: 0 -180px
  .green-but-small:hover
    background-position: 0 -90px
  .green-but-small:active
    color: #dcea8
    background-position: 0 0

  .blue-but-small
    outline: 0
    display: inline-block
    width: 122px
    height: 40px
    color: white
    cursor: pointer
    font-size: 14px
    font-family: 'Arial'
    background: url('/assets/images/market/lk/blue-but.small.png') no-repeat
    background-position: 0 -180px
  .blue-but-small:hover
    background-position: 0 -90px
  .blue-but-small:active
    color: #a8bdc9
    background-position: 0 0

  .red-but-small
    outline: 0
    display: inline-block
    width: 107px
    height: 14px
    padding: 13px 0
    color: white
    cursor: pointer
    font-size: 14px
    line-height: 14px
    text-align: center
    text-decoration: none
    font-family: 'Arial'
    background: url('/assets/images/market/lk/red-but.small.png') no-repeat
    background-position: 0 -180px
  .red-but-small:hover
    background-position: 0 -90px
  .red-but-small:active
    color: #dcea8
    background-position: 0 0
  input.red-but-small
    height: 40px
    padding: 0


  .gray-but-small
      outline: 0
      display: inline-block
      width: 121px
      height: 14px
      padding: 13px 0
      color: white
      cursor: pointer
      font-size: 14px
      line-height: 14px
      text-align: center
      text-decoration: none
      font-family: 'Arial'
      background: url('/assets/images/market/lk/gray-but.small.png') no-repeat
      background-position: 0 -180px
    .gray-but-small:hover
      background-position: 0 0
    .gray-but-small:active
      color: #dcea8
      background-position: 0 -90px


  .select-file-wrap
    width: 300px
    float: left
    overflow: hidden
    .preview
      float: left
      img
        border: 1px solid #b2b2b2
    .selectbox
      width: 200px
      float: left
      padding-left: 20px
      .select-file
        position: relative
        height: 27px
        overflow: hidden
        input
          outline: 0
          position: absolute
          z-index: 2
          cursor: pointer
          top: 0
          left: 0
          display: block
          opacity: 0
          width: 188px
          height: 27px
        .bg
          padding-top: 6px
          position: absolute
          top: 0
          left: 0
          width: 188px
          height: 27px
          background: url('/assets/images/market/lk/gray-but.png') no-repeat
          background-position: 0 -154px
          font-size: 12px
          font-family: 'Arial'
          color: #959595
          text-align: center
      .select-file:hover
        .bg
          background-position: 0 -77px
      .select-file:active
         .bg
          background-position: 0 0
      .descr
        padding-top: 5px
        font-size: 12px
        font-family: 'Arial'
        color: #b4b4b4

  .body-wrap
    .title-wrap
      .center
        .title-inner
          font-size: 30px
          color: #676666
          font-family: 'ParagNorCIta'
          padding-left: 50px
        .back-but
          display: block
          margin-right: 15px
          width: 35px
          height: 35px
          float: left
          text-decoration: none
          background: url('/assets/images/market/lk/back-but.png') no-repeat
          background-position: 0 -85px
        .back-but:hover
          background-position: 0 0

  .error-label
    background: url('/assets/images/market/lk/error-icon.png') no-repeat left center
    padding-left: 50px
    height: 50px
    margin-bottom: 30px
    margin-left: -50px

  .close
    float: right
    width: 17px
    height: 17px
    text-decoration: none
    background: url('/assets/images/market/lk/close-icon.png') no-repeat
    background-position: 0 -67px
  .close:hover
    background-position: 0 0
  .close:active
    background-position: 0 1px

  .popup
    display: none
    position: absolute
    left: 50%
    top: 50%
    z-index: 20000
  .overlay
    display: none
    position: fixed
    top: 0
    left: 0
    z-index: 5
    width: 100%
    height: 100%
    background: black
    opacity: 0.5

  .submit-wrap
    overflow: hidden
    width: 100%

  .popup
    width: 488px
    margin-left: -249px
    background: #e1e1e1
    border-radius(10px)
    .hat
      background: #efefef
      border-bottom: 1px solid #9a9b9b
      border-radius(10px 10px 0 0)
      overflow: hidden
      padding: 10px
    .content
      padding: 25px
      .title
        padding-bottom: 25px
        color: #676666
        font-size: 30px
        font-family: 'ParagNorCIta'
        text-align: center
      .ad-preview
        border: 2px solid #b2b2b2
        background: gray
        margin: 0 auto
      .controls
        text-align: center
        padding-top: 50px

      .note
        color: #b4b4b4
        font-family: Arial
        font-size: 12px
        text-shadow: 1px #eaeaea solid
        padding: 13px 60px
        text-align: center

        .blue
          color: #6290ad
          font-weight: bold

      .textarea
        border: 1px #b2b2b2 solid
        background: #fff
        padding: 1px

        textarea
          width: 404px
          color: #959595
          font-family: Arial
          font-size: 14px
          padding: 12px 15px;height: 160px
          border-top: 1px #d3d3d3 solid
          box-shadow( inset 1px 1px 3px d3d3d3 )
          border-radius( 2px )

  .disable-shop
    .input-wrap
      .left
        display: none

  .disable-ad
    .descr
      text-align: center
      font-size: 12px
      font-family: 'Arial'
      color: #b4b4b4
    a
      color: #4f88a9
      text-decoration: none
    a:hover
      text-decoration: underline

  .hide-content
    display: none

  .err-msg
    padding-top: 5px
    color: #E23131
    font-size: 12px
    font-family: 'Arial'

  .white-board
    position: relative
    display: inline-block
    border-radius(6px)
    background: #efefef
    box-shadow(1px 1px 2px 0 #959595)
    .white-border-top
      border-radius(6px)
      box-shadow(0 -2px 0 0 white)
      overflow: hidden
      .gray-border-bottom
        overflow: hidden
        border-radius(6px)
        box-shadow(0 1px 0 0 gray)

  .triger-wrap
    text-align: center
    span
      font-family: 'Arial'
      font-size: 10px
    span:hover
      cursor: pointer
      color: #5088aa
    .disable-but
      color: #cbcbcb
    .triger
      position: relative
      display: inline-block
      width: 44px
      height: 6px
      background: url('/assets/images/market/lk/triger-bg.png') no-repeat center center
      .circle
        transition( -webkit-transform linear 0.1s )
        transition( transform linear 0.1s )
        position: absolute
        width: 20px
        height: 18px
        top: -6px
        transform( translate3d(-3px, 0, 0))
        background: url('/assets/images/market/lk/circle.png') no-repeat
        background-position: 0 -68px
      .circle:hover
        cursor: pointer
        background-position: 0 0
  .triger-wrap.enabled
    .circle
      transform( translate3d(27px, 0, 0))

  .login-page
    font-size: 14px
    font-family: "Arial"
    width: 600px
    p
      margin: 13px 0
    .lp-title
      font-size: 30px
      color: #676666
      font-family: 'ParagNorCIta'
      text-align: center
      padding: 20px 0 10px
    .block-480
      padding-left: 120px
    .recover-password
      display: inline-block
      float: right
      color: #07a1ca
      text-decoration: none
    .recover-password:hover
      text-decoration: underline

  .lk-list
    .item
      font-family: 'Arial'
      display: block
      padding: 5px 10px
      margin: 2px 0
      text-decoration: none
      .number
        font-size: 12px
        color: #959595
      .name
        padding-left: 100px
        font-size: 24px
        color: #23313d
        font-weight: bold
        font-family: 'PT Sans Narrow'
      .mmart-name
        font-size: 16px
        color: #676666
    .item:hover
      background: #efefef




