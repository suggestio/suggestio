.lk-ad-block-edit-form__preview
  width: 340px
  min-height: 100px
  float: left

.lk-ad-block-edit-form__preview_container
  background: url('/assets/images/ld-ad-block-size-grid.png') no-repeat left top
  float: left
  min-height: 140px
  min-width: 140px
  padding-left: 23px
