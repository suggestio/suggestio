/* Кнопки */
.green-but-big
  outline: 0
  display: block
  width: 210px
  height: 14px
  color: white
  cursor: pointer
  font-size: 14px
  line-height: 14px
  text-decoration: none
  text-align: center
  padding: 13px 0
  font-family: 'Arial'
  background: url('/assets/images/market/lk/green-but.big.png') no-repeat
  background-position: 0 -180px
.green-but-big:hover
  background-position: 0 -90px
.green-but-big:active
  color: #dcea8
  background-position: 0 0

.sm-note
  position: relative
  padding: 10px 0px
  font-size: 12px
  color: #959595

.sm-note__q-mark
  color: #208bae
  font-family: Arial
  font-size: 13px
  padding: 0px
  cursor: help
  border-bottom: 1px #208bae solid

.sm-note__message
  display: none

.sm-note__link
  color: #5088aa

.sm-light-text
  color: #959595

.g-sm-strong
  font-size: 14px
  font-weight: bold

.superuser-link
  font-size: 10px
  color: #999

.g-price
  font-family: 'PT Sans Narrow','sans-serif'
  font-size: 24px
  color: #676666

.sm-large-button
  font-family: 'PFDinTextCompPro-Medium'
  border: 1px #9c9c9c solid
  color: #676666
  text-decoration: none
  font-size: 30px
  display: inline-block
  border-radius( 4px )

  span
    border-radius( 4px )
    padding: 4px 15px 0px 15px
    display: inline-block
    background: #e2e1e1
    border-bottom: 1px #cbcbcb solid
    border-top: 1px #f7f7f7 solid
    text-shadow( 1px 1px 1px #fff )


.sm-large-button:hover
  color: #4f88a9

.sm-large-button:active
  color: #676666

  span
    background: #cfcfcf
    box-shadow(inset 0px 0px 14px #a9a9a9)

.sm-large-button_disabled
  opacity: 0.5
  cursor: default

.sm-large-button_disabled:hover
  color: #676666

.sm-large-button_disabled:active
  color: #676666

  span
    background: #e2e1e1
    box-shadow: none


/* Разные окошки, которые нужно отображать поверх всего */
.lk-overlay-data
  display: none
  position: fixed
  left: 0px
  top: 0px
  width: 100%
  height: 100%
  z-index: 10000
  overflow-y: scroll
  background: rgba(0, 0, 0, 0.6)

.lk-overlay-data__window
  width: 700px
  margin-top: 50px
  margin-bottom: 50px
  background: #e1e1e1

/* Окошко для кадрирования картинок */
.market-crop-tool__container
