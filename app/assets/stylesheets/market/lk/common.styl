/* Разные окошки, которые нужно отображать поверх всего */
.lk-overlay-data
  display: none
  position: fixed
  left: 0px
  top: 0px
  width: 100%
  height: 100%
  z-index: 10000
  overflow-y: scroll

.lk-overlay-data__window
  width: 700px
  margin-top: 50px
  margin-bottom: 50px
  background: #e1e1e1

/* Окошко для кадрирования картинок */
.market-crop-tool__container
  border: 1px #ccc solid
  overflow: hidden

/* Мелкая ссылка */
.lk-small-link
  font-size: 12px

.lk-small-link_gray
  color: #333

/* Инпут для загрузки картинки */
.lk-file-input
  position: relative
  height: 30px
  display: inline-block

.lk-file-input__div
  cursor: pointer
  position: absolute
  left: 0px
  top: 0px
  color: #959595
  font-size: 12px
  border: 1px #b2b2b2 solid
  border-radius( 20px )

.lk-file-input__div-inner
  cursor: pointer
  border-top: 1px #fff solid
  border-radius( 20px )
  padding: 4px 10px
  width: 160px
  text-align: center
  background: #e2e2e2

  background: -moz-linear-gradient(top,  #efefef 0%, #e2e2e2 100%)
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#efefef), color-stop(100%,#e2e2e2));
  background: -webkit-linear-gradient(top,  #efefef 0%,#e2e2e2 100%)
  background: -o-linear-gradient(top,  #efefef 0%,#e2e2e2 100%)
  background: -ms-linear-gradient(top,  #efefef 0%,#e2e2e2 100%)
  background: linear-gradient(to bottom,  #efefef 0%,#e2e2e2 100%)

.lk-file-input:hover

  .lk-file-input__div-inner
    color: #2e2e2e
    background: #f6f6f6

.lk-file-input:active

  .lk-file-input__div-inner
    background: -moz-linear-gradient(bottom,  #efefef 0%, #e2e2e2 100%)
    background: -webkit-gradient(linear, left bottom, left top, color-stop(0%,#efefef), color-stop(100%,#e2e2e2));
    background: -webkit-linear-gradient(bottom,  #efefef 0%,#e2e2e2 100%)
    background: -o-linear-gradient(bottom,  #efefef 0%,#e2e2e2 100%)
    background: -ms-linear-gradient(bottom,  #efefef 0%,#e2e2e2 100%)
    background: linear-gradient(to top,  #efefef 0%,#e2e2e2 100%)

.lk-file-input__input
  opacity: 0
  cursor: pointer

/* Инструмент для установки выравнивания текста */
.align-editor
  width: 81px
  border-bottom: 1px #686868 solid

.align-editor__button
  background: #e0e0e0 url('/assets/images/align-editor-sprite.png') no-repeat 4px 6px
  width: 25px
  height: 23px
  float: left
  border-left: 1px #f5f5f5 solid
  border-right: 1px #cbcbcb solid
  cursor: pointer

.align-editor__button_active
  background-color: #d6d3d3

.align-editor__button_center
  background-position: -23px 6px

.align-editor__button_right
  background-position: -50px 6px

/* Поля ввода */
.lk-text-field
  border: 1px #b2b2b2 solid
  padding: 1px

  div
    border-top: 1px #d3d3d3 solid
    border-left: 1px #fbfbfb solid
    border-right: 1px #fbfbfb solid
    padding: 10px 15px
    background: #fff

    input,
    textarea
      width: 100%
      outline: none

.lk-color-select
  width: 100px

.lk-color-select__label,
.lk-color-select__color
  float: left

.lk-color-select__label
  padding-top: 6px
  width: 54px
  color: #959595
  font-size: 12px
  font-family: Arial

.lk-color-select__label_hidden
  display: none

.lk-color-select__color
  cursor: pointer
  position: relative
  width: 27px
  height: 27px
  border: 1px #b2b2b2 solid

.lk-color-select__color_custom
  div
    position: absolute
    left: 0px
    top: 0px
