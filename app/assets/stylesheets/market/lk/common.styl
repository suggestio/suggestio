/* Поля ввода */
.lk-text-field
  border: 1px #b2b2b2 solid
  padding: 1px

  div
    border-top: 1px #d3d3d3 solid
    border-left: 1px #fbfbfb solid
    border-right: 1px #fbfbfb solid
    padding: 10px 15px
    background: #fff

    input,
    textarea
      width: 100%
      outline: none

.lk-color-select
  width: 100px

.lk-color-select__label,
.lk-color-select__color
  float: left

.lk-color-select__label
  padding-top: 6px
  width: 45px
  color: #959595
  font-size: 12px
  font-family: Arial

.lk-color-select__color
  cursor: pointer
  position: relative
  width: 27px
  height: 27px
  border: 1px #b2b2b2 solid

.lk-color-select__color_custom
  div
    position: absolute
    left: 0px
    top: 0px
    width: 27px
    height: 27px
    background: lk_sprite
    background-position: left -190px

.lk-field-w-colorpicker__left,
.lk-field-w-colorpicker__right
  float: left


/* Общие стили */
.lk-edit-form__block
  margin: 10px 0px

.lk-edit-form__block_title
  background: #dad9d9
  padding: 17px 11px
  color: #676666
  font-weight: bold

.lk-edit-form__block_body
  background: #efefef
  padding: 20px 22px

.lk-h2
  color: #676666
  font-size: 14px
  font-weight: bold
  padding: 10px 0px

/* Список блоков в виде иконок */
.lk-edit-form__blocks-list-icons
  padding: 0px

.blocks-list-icons__single-icon
  float: left
  cursor: pointer
  background: #efefef
  width: 60px
  height: 60px
  margin-right: 10px
  border: 1px transparent solid
  background-image: url('/assets/images/block-icons-sprite.png')

.blocks-list-icons__single-icon_active
  cursor: default
  border-color: #a2a2a2
  background-color: #e1e1e1
  box-shadow( inset 0px 2px 3px #a8acaf )

.block-list-icon-1
  background-position: left -959px

.block-list-icon-2
  background-position: left -1039px

.block-list-icon-3
  background-position: left -879px

.block-list-icon-4
  background-position: left -3259px


