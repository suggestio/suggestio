border-radius(v)
  -webkit-border-radius:v
  border-radius:v

box-shadow(v)
  -webkit-box-shadow: v
  -moz-box-shadow:    v
  box-shadow:         v

text-shadow(v)
  -webkit-text-shadow: v
  -moz-text-shadow:    v
  text-shadow:         v


transition(v)
  -webkit-transition: v
  transition: v

transform(v)
  -webkit-transform: v
  transform: v

.clear
  clear: both

