.gray-but-wrap
  position: relative
  border: 1px solid #9a9b9b
  border-radius(6px)
  display: inline-block
  padding: 1px 0

  .white-border-top
    border-radius(6px)
    box-shadow(0 -1px 0 0 white)
    overflow: hidden

    .btn
      background: none
      border: none
      display: inline-block
      padding: 16px 18px
      color: #676666
      cursor: pointer
      font-size: 30px
      text-decoration: none
      text-align: center
      font-family: 'PFDinTextCompPro-Medium'
      box-shadow(0 1px 0 0 #B5B6B6)
      text-shadow(1px 1px 1px #F8F8F8)
.gray-but-wrap:hover
  border: 1px solid #4f88a9
  .btn
    color: #4f88a9
.gray-but-wrap:active
  border: 1px solid #9a9b9b
  box-shadow(inset 0 0 40px 0 #9a9b9b)

  .white-border-top
    box-shadow: none

    .btn
      color: #676666


